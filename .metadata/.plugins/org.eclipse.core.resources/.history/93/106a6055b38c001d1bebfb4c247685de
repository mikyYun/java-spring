package reviewCh16;

public class EmployeeTest {

	public static void main(String[] args) {

		Employee employeeA = new Employee();
//		System.out.println(employeeA.employeeId) => employeeId is private. cannot get value by name
		System.out.println(employeeA.getEmployeeId()); // auto increased serialNumber
		System.out.println(employeeA.getEmployeeName()); // initial null
		System.out.println(employeeA.getDepartment()); // initial null
		
		employeeA.setEmployeeName("Mike");
		employeeA.setDepartment("Dev");
		System.out.println(employeeA.getEmployeeName());
		System.out.println(employeeA.getDepartment()); 
		
		
	}

}

package ch16;

public class EmployeeTest {

	public static void main(String[] args) {

		Employee employeeA = new Employee();
		employeeA.setEmployeeName("Mike");

//		System.out.println(employeeA.serialNum);
		System.out.println(Employee.serialNum);
		
